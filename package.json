{
  "name": "itunes-remote",
  "version": "2.0.1",
  "description": "Control iTunes via CLI",
  "license": "MIT",
  "repository": "mischah/itunes-remote",
  "author": {
    "name": "Michael KÃ¼hnel",
    "email": "mail@michael-kuehnel.de",
    "url": "http://michael-kuehnel.de"
  },
  "bin": "cli.js",
  "preferGlobal": true,
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "lint": "xo",
    "test": "mocha",
    "preparepublish": "npm run coveralls",
    "posttest": "npm run lint && npm run coverage",
    "coverage": "mocha -r blanket -R html-cov > coverage.html",
    "coveralls": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 mocha --require blanket --reporter mocha-lcov-reporter | coveralls"
  },
  "files": [
    "index.js",
    "cli.js",
    "lib"
  ],
  "keywords": [
    "cli-app",
    "cli",
    "itunes",
    "mac",
    "osx",
    "remote",
    "Music",
    "Terminal",
    "OS X"
  ],
  "dependencies": {
    "chalk": "^1.1.1",
    "js-function-string": "^1.0.0",
    "log-symbols": "^1.0.2",
    "osascript": "^1.2.0",
    "require-dir": "^0.3.0",
    "vorpal": "^1.5.5"
  },
  "devDependencies": {
    "blanket": "^1.2.1",
    "chai": "^3.4.1",
    "coveralls": "^2.11.6",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0",
    "xo": "^0.12.1"
  },
  "xo": {
    "envs": [
      "node",
      "mocha"
    ]
  },
  "config": {
    "blanket": {
      "pattern": [
        "index.js",
        "cli.js",
        "lib"
      ],
      "data-cover-never": [
        "node_modules",
        "lib"
      ]
    }
  }
}
